generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model clients {
  numFidelite    String      @id 
  nom            String
  prenom         String
  mail           String
  motDePasse     String
  nbrePoints     Int?
  derniereVisite DateTime?@updatedAt
  
        
}
 model product {
  Date           DateTime   @id
  produit        String?
  prix           Int?
  imageUrl       String?

  
 
}  